[[source]]
url = "https://pypi.org/simple"
verify_ssl = true
name = "pypi"

[packages]
fastapi = "*"
pydantic = "*"
uvicorn = "*"

[dev-packages]

[requires]
python_version = "3.10"

[scripts]
runserver = "uvicorn main:app --reload"
start = "curl -X GET http://127.0.0.1:8000/start"
move = "curl -X POST -H \"Content-Type: application/json\" -d '{\"direction\": \"right\"}' http://127.0.0.1:8000/move"
